class Node_Double
{
	double data
	ptr<Node_Double> next
	ptr<Node_Double> previous
	ptr<Node_Double> _nullptr_ND // nullptr for Node_Double

	bool hasNext()
	{
		return next == _nullptr_ND
	}

	bool hasPrevious()
	{
		return previous == _nullptr_ND
	}
	
}

class LinkedList_Double
{
	ptr<Node_Double> _front, _back
	ptr<Node_Double> _nullptr_ND
	int _size

	void push_front(double data)
	{
		Node_Double newNode
		
		newNode.data = data
		ptr<Node_Double> node = &newNode
		
		
		if (_size == 0)
		{
			_front = node
			_back = node
		}
		else
		{
			(*_front).previous = node;
			(*node).next = _front
			_front = node
		}
		++<_size
	}

	void push_back(double data)
	{
		Node_Double newNode
		newNode.data = data
		ptr<Node_Double> node = &newNode
		
		if (_size == 0)
		{
			_front = node
			_back = node
		}
		else
		{
			(*_back).next = node;
			(*node).previous = _back
			_back = node
		}
		++<_size
	}
	
	void pop_front()
	{
		if ( _size == 0 )
		{
			print("\nList is empty. exit")
			exit
		}
		
		ptr<Node_Double> buf = _front
		_front = (*_front).next
		if (_front != _nullptr_ND)
		{
			(*_front).previous = _nullptr_ND
		}
		else
		{		
			_back = _nullptr_ND
		}
		(*buf).next = _nullptr_ND
		--<_size
	}
	
	void pop_back()
	{
		if ( _size == 0 )
		{
			print("\nList is empty. exit")
			exit
		}
		
		ptr<Node_Double> buf = _back
		_back = (*_back).previous
		if (_back != _nullptr_ND)
		{
			(*_back).next = _nullptr_ND
		}
		else
		{
			_front = _nullptr_ND
		}
		(*buf).previous = _nullptr_ND
		--<_size
	}
	
	double front()
	{
		if (_size == 0)
		{
			print("\nList is empty. exit")
			exit
		}
		return (*_front).data
	}
	
	double back()
	{
		if (_size == 0)
		{
			print("\nList is empty. exit")
			exit
		}		
		return (*_back).data
	}
	
	int size()
	{
		return _size
	}
	bool empty()
	{
		return _size == 0
	}
	
	void print_list()
	{
		ptr<Node_Double> cur = _front
		
		print("-")
		while (cur != _nullptr_ND)
		{
			print(" " + (*cur).data + " -")
			cur = (*cur).next
		}
	}
}
	
LinkedList_Double List

List.push_back(1)
List.push_back(2)
List.push_back(3)
List.push_back(4)
List.push_back(5)

print("Size: " + List.size() + "\nList: ")
List.print_list()

List.pop_back()
List.pop_front()

print("\n\nSize: " + List.size() + "\nList: ")
List.print_list()

while (!List.empty())
	List.pop_back()

print("\n\nSize: " + List.size() + "\nList: ")
List.print_list()

